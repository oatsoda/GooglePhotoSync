{
  "GoogleSettings": {
    "GooglePhotosApiBaseUrl": "https://photoslibrary.googleapis.com/",
    "GoogleClientId": "",
    "GoogleClientSecret": "",
    "GooglePhotoScope": "https://www.googleapis.com/auth/photoslibrary",
    "GoogleTokenUrl": "https://oauth2.googleapis.com/token"
  },
  "LocalSettings": {
    "LocalFolderRoot": "",
    "IgnoreFolderStartingWith": [ "_", "0.", "1.", "2." ],
    "MaxFileSizeBytes": 49000000,
    "ImageExtensions": [
      ".bmp",
      ".gif",
      ".jpg",
      ".jpeg",
      ".png"
    ],
    "VideoExtensions": [
      ".3gp",
      ".avi",
      ".mov",
      ".mp4",
      ".mpg",
      ".wmv"
    ]
  },
  "SyncSettings": {
    "PromptBeforeEachAlbumSync": true,
    "BatchSize": 5,
    "ParallelUploads": 5
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "System": "Warning",
      "GooglePhotoSync": "Debug"
    }
  }
}
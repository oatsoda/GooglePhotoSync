{
  "GoogleSettings": {
    "GooglePhotosApiBaseUrl": "https://photoslibrary.googleapis.com/",
    "GoogleClientId": "",
    "GoogleClientSecret": "",
    "GooglePhotoScope": "https://www.googleapis.com/auth/photoslibrary"
  },
  "LocalSettings": {
    "LocalFolderRoot": "",
    "IgnoreFolderStartingWith": [ "_", "0.", "1.", "2." ],
    "MaxFileSizeBytes": 45000000,
    "ImageExtensions": [
      ".bmp",
      ".gif",
      ".jpg",
      ".jpeg",
      ".png"
    ],
    "VideoExtensions": [
      ".3gp",
      ".avi",
      ".mov",
      ".mp4",
      ".mpg",
      ".wmv"
    ]
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "System": "Warning",
      "GooglePhotoSync": "Debug"
    }
  }
}